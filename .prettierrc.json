{
	"tabWidth": 2,
	"printWidth": 80,
	"trailingComma": "none",
	"semi": false,
	"singleQuote": true,
	"endOfLine": "auto",
	"overrides": [
		{
		  "files": ["*.json5", "*.json"],
		  "options": {
			"singleQuote": false,
			"quoteProps": "preserve"
		  }
		},
		{
		  "files": ["*.yml"],
		  "options": {
			"singleQuote": false
		  }
		}
	  ]
}
